{
  "expo": {
    "scheme": "tasklender",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-tasklender.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/music/so-proud-notification.wav"
          ]
        }
      ]
    ],
    "name": "Tasklender",
    "description": "Introducing Tasklender, an innovative application designed to offer invaluable support to individuals leading fast-paced and demanding lifestyles. Tasklender serves as an indispensable tool, catering to the needs of busy and stressed individuals by providing intuitive and efficient organizational and planning resources.",
    "owner": "Winson Cloud Storage",
    "slug": "tasklender-app",
    "version": "1.0.0",
    "icon": "./assets/icon-tasklender.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-tasklender.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber" : "0",
      "supportsTablet": true,
      "bundleIdentifier": "tasklender.technopedie.com"
    },
    "android": {
      "versionCode": 0,
      "package": "tasklender.technopedie.com",
      "permissions": ["RECEIVE_BOOT_COMPLETED", "WRITE_CONTACTS", "READ_CONTACTS", "CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon-tasklender.png"
    }
  }
}
